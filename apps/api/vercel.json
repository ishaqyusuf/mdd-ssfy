{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/api/hono-trpc/backlogs.getByClassroom",
      "permanent": false
    }
  ],
  "buildCommand": "bun run build:vc",
  "regions": ["bom1"]
}
